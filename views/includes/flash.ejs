<% if(success && success.length) {%>
<div class="mt-3 alert alert-success flash  alert-dismissible fade show" role="alert">
    <%= success %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
  <% } %>
<% if(error && error.length) {%>
<div class="mt-3 alert alert-danger flash alert-dismissible fade show" role="alert">
    <%= error %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
  <% } %>